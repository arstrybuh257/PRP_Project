@model GainBargain.WEB.Models.MarketVM

@{
    ViewBag.Title = Model.Name;
}

<div class="container-fluid w-75">
    <div class="row p-3">
        <div class="col-5">
            <img class="img-fluid" src="@Model.MarketLogoUrl"/>
        </div>
        <div class="col-7">
            <h2 class="mt-3 mb-2">@Model.Name</h2>
            <p>@Model.Description</p>
        </div>
    </div>
</div>
